{
  "name": "@deskpro-apps/klaviyo",
  "title": "Klaviyo",
  "description": "Integrate with Klaviyo to manage your customers' personalized experiences across email, SMS, mobile push, and more",
  "version": "1.0.9",
  "scope": "agent",
  "isSingleInstall": false,
  "hasDevMode": true,
  "serveUrl": "https://apps-cdn.deskpro-service.com/__name__/__version__",
  "secrets": "",
  "appStoreUrl": "https://www.deskpro.com/product-embed/apps/klaviyo",
  "targets": [
    {
      "target": "user_sidebar",
      "entrypoint": "index.html"
    }
  ],
  "entityAssociations": {
    "linkedKlaviyoProfiles": {
      "entity": "person",
      "type": "external_id",
      "title": "Klaviyo Profiles"
    }
  },
  "settings": {
    "use_deskpro_saas": {
      "title": "One-Click Installation",
      "type": "boolean",
      "default": true,
      "isRequired": false,
      "isBackendOnly": false,
      "order": 5
    },
    "use_api_key": {
      "title": "Use API Token",
      "type": "boolean",
      "isRequired": false,
      "isBackendOnly": false,
      "default": false,
      "condition": "settings.use_deskpro_saas != true",
      "order": 10
    },
    "api_key": {
      "title": "API Key",
      "type": "string",
      "isRequired": false,
      "isBackendOnly": true,
      "condition": "settings.use_deskpro_saas != true && settings.use_api_key == true",
      "order": 20
    },
    "verify_settings": {
      "title": "",
      "type": "app_embedded",
      "options": {
        "entrypoint": "#/admin/verify_settings",
        "height": "30px"
      },
      "isRequired": false,
      "isBackendOnly": true,
      "condition": "settings.use_deskpro_saas != true && settings.use_api_key == true",
      "order": 30
    },
    "client_id": {
      "title": "Client ID",
      "type": "string",
      "isRequired": false,
      "isBackendOnly": false,
      "condition": "settings.use_deskpro_saas != true && settings.use_api_key != true",
      "order": 40
    },
    "client_secret": {
      "title": "Client Secret",
      "type": "string",
      "isRequired": false,
      "isBackendOnly": true,
      "condition": "settings.use_deskpro_saas != true && settings.use_api_key != true",
      "order": 50
    },
    "callback_url": {
      "title": "Callback URL",
      "type": "app_embedded",
      "options": {
        "entrypoint": "#/admin/callback"
      },
      "isRequired": false,
      "isBackendOnly": true,
      "condition": "settings.use_deskpro_saas != true && settings.use_api_key != true",
      "order": 60
    }
  },
  "proxy": {
    "whitelist": [
      {
        "url": "https://a.klaviyo.com/api/.*",
        "methods": [
          "GET",
          "POST",
          "PATCH"
        ],
        "timeout": 20
      }
    ]
  }
}